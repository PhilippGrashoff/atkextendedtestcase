{
  "name": "philippgrashoff/atkextendedtestcase",
  "description": "TestCase with SQLite persistence",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.2"
    }
  },
  "require": {
    "atk4/data": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.5",
    "phpstan/phpstan": "1.*"
  },
  "autoload": {
    "psr-4": {
      "atkextendedtestcase\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atkextendedtestcase\\tests\\": "tests"
    }
  }
}
